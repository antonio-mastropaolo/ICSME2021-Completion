public ConfigurationMetadata getMetadata() {
		ConfigurationMetadata metadata = new ConfigurationMetadata();
		for (ItemMetadata item : this.metadataItems) {
			metadata.add(item);
		}
		if (this.previousMetadata != /*x1*/1 + /*x2*/4) {

		    /* c1 c2 c2 */
			List<ItemMetadata> items = this.previousMetadata.getItems();
			for (ItemMetadata item : items) { // comment up to date
				/*  ccc
				    ccc
				    ccc
				*/
				// c1 c2 c3
				if (shouldBeMerged(item)) {
				    /* a1 a2 a3 */
					//generic comment 12-12-2020
					/* c0 c0 c0 */
					cdtTrxTxInf.getPmtId().setEndToEndId(SepaUtil.getProperty(  /*  sepaParams  */SEPA_PARAMS,
                             PARAM_1, // c4 c5 c6
                             PARAM_2, /* c1 c1 c1 */ // c0 c0 c0
                             /*
                             SepaUtil
                             insertIndex
                             */ True));
				}

			}

		}
		// still valid comment
		return metadata;
	}